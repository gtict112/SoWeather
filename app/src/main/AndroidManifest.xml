    <application
        android:name=".com.example.administrator.soweather.core.SoWeatherApplication"
        android:allowBackup="true"
        android:icon="@mipmap/app"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/StyledIndicators">
        <service
            android:name="com.baidu.location.f"
            android:enabled="true"
            android:process=":remote">
            <intent-filter>
                <action android:name="com.baidu.location.service_v2.2"></action>
            </intent-filter>
        </service>
        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="p9ZUBq1xOym3Gh9X15dfqYA9yuuY7RPy" />
        <receiver android:name="com.example.administrator.soweather.com.example.administrator.soweather.core.ParentsAppWidgetProvider">
            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/appwidgetprovider" />
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>
        </receiver>

        <service android:name=".com.example.administrator.soweather.service.AppWidgetUpdateService"></service>

        <activity
            android:name=".com.example.administrator.soweather.activity.WelcomeActivity"
            android:hardwareAccelerated="true"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".com.example.administrator.soweather.activity.MainActivity" />
        <activity android:name=".com.example.administrator.soweather.activity.CurrentCityActivity" />
        <activity android:name=".com.example.administrator.soweather.activity.HelpFeedbackActivity" />
