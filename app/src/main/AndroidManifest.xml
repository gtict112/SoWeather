        android:allowBackup="true"
        android:icon="@mipmap/app"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name="com.baidu.location.f"
            android:enabled="true"
            android:process=":remote" />
        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="dzb78OZ403ijf5DrDuV9H7v2UZjAOWQ7" />

        <receiver android:name="com.example.administrator.soweather.com.example.administrator.soweather.core.ParentsAppWidgetProvider">
            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/appwidgetprovider" />
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>
        </receiver>

        <service android:name=".com.example.administrator.soweather.service.AppWidgetUpdateService"></service>

        <activity
            android:name=".com.example.administrator.soweather.activity.SpanActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".com.example.administrator.soweather.activity.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".com.example.administrator.soweather.activity.CurrentCityActivity"
            android:label="选择城市"
            android:screenOrientation="portrait" />
        <activity
            android:name=".com.example.administrator.soweather.activity.HelpFeedbackActivity"
            android:label="留言与反馈"
            android:screenOrientation="portrait" />
        <activity
            android:name=".com.example.administrator.soweather.activity.CurrenCityTwoActivity"
