            android:name=".com.example.administrator.soweather.activity.CurrenCityThreeActiivty"
            android:label="选择城市"
            android:screenOrientation="portrait" />
        <activity
            android:name=".com.example.administrator.soweather.activity.AboutMoreActivity"
